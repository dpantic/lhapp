<%- model_class = Amm -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:ac_type) %>:</strong></dt>
  <dd><%= @amm.ac_type %></dd>
  <dt><strong><%= model_class.human_attribute_name(:mm_referenz) %>:</strong></dt>
  <dd><%= @amm.mm_referenz %></dd>
  <dt><strong><%= model_class.human_attribute_name(:bauteil) %>:</strong></dt>
  <dd><%= @amm.bauteil %></dd>
  <dt><strong><%= model_class.human_attribute_name(:eff) %>:</strong></dt>
  <dd><%= @amm.eff %></dd>
</dl>

<div class="form-actions">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              amms_path, :class => 'btn'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_amm_path(@amm), :class => 'btn' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              amm_path(@amm),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
</div>
